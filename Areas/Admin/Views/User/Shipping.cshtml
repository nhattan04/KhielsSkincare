@model IEnumerable<KhielsSkincare.Models.Shipping> 

@{
    Layout = "_LayoutAdmin";
    ViewData["Title"] = "Danh sách Giao Hàng";
}

<div class="container-fluid">
    <h5 class="h3 mb-2 text-gray-800">@ViewData["Title"]</h5>

    <table class="table table-bordered">
        <thead>
            <tr>
                <th>ID</th>
                <th>Họ</th>
                <th>Tên</th>
                <th>Số điện thoại</th>
                <th>Địa chỉ</th>
                <th>Thành phố</th>
                <th>Mã đơn hàng</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var shipping in Model)
            {
                <tr>
                    <td>@shipping.Id</td>
                    <td>@shipping.LastName</td>
                    <td>@shipping.FirstName</td>
                    <td>@shipping.PhoneNumber</td>
                    <td>@shipping.AddressLine</td>
                    <td>@shipping.City</td>
                    <td>@shipping.OrderCode</td>
                </tr>
            }
        </tbody>
    </table>
</div>

